    "@angular/platform-browser-dynamic": "6.0.6",
    "@angular/router": "6.0.6",
    "@types/detect-browser": "2.0.1",
    "core-js": "2.5.5",
    "detect-browser": "2.4.0",
    "dexie": "2.0.3",
    "font-awesome": "4.7.0",
